@model Backend5.Models.Doctor
@{
    ViewBag.Title = "Details";
}
<h1>@ViewBag.Titles</h1>
<h2>Doctor</h2>
<div>
    <dl class="dl-horizontal">
        <dt>Name</dt>
        <dd>@Model.Name</dd>
        <dt>Speciality</dt>
        <dd>@Model.Speciality</dd>
    </dl>
</div>
<div>
    <a asp-action="Index" asp-controller="Doctors">Back to List</a>|
    <a asp-action="Edit" asp-controller="Doctors" asp-route-id="@Model.Id">Edit</a>
</div>
