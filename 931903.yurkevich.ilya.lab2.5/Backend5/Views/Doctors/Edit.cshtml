@model Backend5.Models.Doctor
@{
    ViewBag.Title = "Edit";
}
<h1>@ViewBag.Titles</h1>
<h2>Doctor</h2>
<form asp-action="edit" asp-controller="Doctors" asp-route-id="@Model.Id">
    <div class="form-group">
        <b> <label asp-for="Name" class="control-label">Name</label></b>
        <input type="text" asp-for="Name" class="form-control" />
        <span asp-validation-for="Name"></span>
    </div>
    <div class="form-group">
        <b><label asp-for="Speciality" class="control-label">Speciality</label></b>
        <input type="text" asp-for="Speciality" class="form-control" />
        <span asp-validation-for="Speciality"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Edit" class="btn btn-outline-dark" />
    </div>
    <div>
        <a asp-action="Index" asp-controller="Doctors">Back to List</a>
    </div>
</form>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}